<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.ymd.libsys.controller.LoginController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <Label layoutX="52.0" layoutY="66.0" text="Username" />
        <Label layoutX="54.0" layoutY="123.0" text="Password" />
        <TextField fx:id="tf" layoutX="167.0" layoutY="61.0" />
        <PasswordField fx:id="pf" layoutX="167.0" layoutY="118.0" />
        <Button fx:id="btn" layoutX="280.0" layoutY="168.0" mnemonicParsing="false" onAction="#submit" prefHeight="74.0" prefWidth="96.0" text="Submit" />
    </children>

</AnchorPane>
