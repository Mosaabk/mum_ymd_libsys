<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="626.0" prefWidth="621.0" stylesheets="@main.css" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ymd.libsys.controller.MainMenuController">
   <children>
      <BorderPane prefHeight="626.0" prefWidth="625.0" />
      <Button fx:id="btnAddCopy" layoutX="410.0" layoutY="220.0" mnemonicParsing="false" onAction="#addCopy" prefHeight="80.0" prefWidth="190.0" text="Add A Copy" />
      <Button fx:id="btnAddMem" layoutX="10.0" layoutY="130.0" minHeight="80.0" mnemonicParsing="false" onAction="#addNewMember" prefWidth="190.0" text="Add A New Member" />
      <Button fx:id="btnAddBook" layoutX="210.0" layoutY="220.0" minHeight="80.0" mnemonicParsing="false" onAction="#addBook" prefWidth="190.0" text="Add A New Book" />
      <Button fx:id="btnCheckout" layoutX="410.0" layoutY="130.0" minHeight="80.0" mnemonicParsing="false" onAction="#checkoutBook" prefWidth="190.0" text="Checkout A Book" />
      <Button fx:id="btnEditMem" layoutX="210.0" layoutY="130.0" minHeight="80.0" mnemonicParsing="false" onAction="#editMember" prefWidth="190.0" text="Edit An Exsisting Member" />
      <Button fx:id="btnLogout" layoutX="210.0" layoutY="320.0" minHeight="80.0" mnemonicParsing="false" onAction="#logOut" prefWidth="190.0" text="Logout" />
      <Button fx:id="btnShowMembers" layoutX="10.0" layoutY="220.0" minHeight="80.0" mnemonicParsing="false" onAction="#showMembers" prefWidth="190.0" text="Show Member List" />
   </children>
   
</AnchorPane>
