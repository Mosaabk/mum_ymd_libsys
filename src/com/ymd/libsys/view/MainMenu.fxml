<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="626.0" prefWidth="621.0" xmlns="http://javafx.com/javafx" 
	xmlns:fx="http://javafx.com/fxml"
  fx:controller="com.ymd.libsys.controller.MainMenuController"
  stylesheets="@main.css">
   <children>
      <BorderPane prefHeight="626.0" prefWidth="625.0" />
      <Button fx:id="btnAddCopy" layoutX="210.0" layoutY="310.0" mnemonicParsing="false" onAction="#addCopy" prefHeight="80.0" prefWidth="190.0" text="Add A Copy" />
      <Button fx:id="btnAddMem" layoutX="210.0" layoutY="40.0" minHeight="80.0" mnemonicParsing="false" onAction="#addNewMember" prefWidth="190.0" text="Add A New Member" />
      <Button fx:id="btnAddBook" layoutX="210.0" layoutY="220.0" minHeight="80.0" mnemonicParsing="false" onAction="#addBook" prefWidth="190.0" text="Add A New Book" />
      <Button fx:id="btnCheckout" layoutX="210.0" layoutY="400.0" minHeight="80.0" mnemonicParsing="false" onAction="#checkoutBook" prefWidth="190.0" text="Checkout A Book" />
      <Button fx:id="btnEditMem" layoutX="210.0" layoutY="130.0" minHeight="80.0" mnemonicParsing="false" onAction="#editMember" prefWidth="190.0" text="Edit An Exsisting Member" />
      <Button fx:id="btnLogout" layoutX="210.0" layoutY="490.0" minHeight="80.0" mnemonicParsing="false" onAction="#logOut" prefWidth="190.0" text="Logout" />
   </children>
   
</AnchorPane>
